generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id            Int      @id @default(autoincrement())
  code          String   @unique
  targetUrl     String
  totalClicks   Int      @default(0)
  lastClickedAt DateTime?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
